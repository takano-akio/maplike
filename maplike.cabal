license:             PublicDomain
license-file:        LICENSE
author:              Takano Akio
maintainer:          aljee@hyper.cx
-- copyright:           
category:            Data
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

source-repository head
  type:              git
  location:          http://github.com/takano-akio/maplike

library
  exposed-modules:     Data.Maplike.Class, Data.Maplike.Lazy, Data.Maplike.Strict
  build-depends:       base >=4.8 && <5, hashable >=1.2 && <1.4, unordered-containers >=0.2 && <0.3, containers >=0.5 && <0.7
